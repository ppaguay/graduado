<pretty-config xmlns="http://ocpsoft.org/schema/rewrite-config-prettyfaces" 
               xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
               xsi:schemaLocation="http://ocpsoft.org/schema/rewrite-config-prettyfaces
                      http://ocpsoft.org/xml/ns/prettyfaces/rewrite-config-prettyfaces.xsd">
    <!--autentication fin y error-->
    <url-mapping id="login">
        <pattern value="/login"/>
        <view-id value="/app/views/autentication/login.xhtml" />
    </url-mapping>
 
    <url-mapping id="404">
        <pattern value="/404"/>
        <view-id value="/app/views/error/404.xhtml" />
    </url-mapping>
    <url-mapping id="500">
        <pattern value="/500"/>
        <view-id value="/app/views/error/500.xhtml" />
    </url-mapping>
    <url-mapping id="logout">
        <pattern value="/logout"/>
        <view-id value="/app/views/autentication/logout.xhtml" />
    </url-mapping>
    <!--fin autentication fin y error-->
    <!--admin-->
    <url-mapping id="admin">
        <pattern value="/admin"/>
        <view-id value="/app/views/administrador/admin.xhtml" />
    </url-mapping>   
    <url-mapping id="empresas">
        <pattern value="/empresas"/>
        <view-id value="/app/views/administrador/empresas.xhtml" />
    </url-mapping>   
    <url-mapping id="ofertaLaboral">
        <pattern value="/ofertaLaboral"/>
        <view-id value="/app/views/administrador/ofertaLaboral.xhtml" />
    </url-mapping>   
    <url-mapping id="graduados">
        <pattern value="/graduados"/>
        <view-id value="/app/views/administrador/graduados.xhtml" />
    </url-mapping>   
    <url-mapping id="historiaLaboral">
        <pattern value="/historiaLaboral"/>
        <view-id value="/app/views/administrador/historiaLaboral.xhtml" />
    </url-mapping>   
    <!--fin admin-->
    <!--empresa-->
    <url-mapping id="adminEmpresa">
        <pattern value="/adminEmpresa"/>
        <view-id value="/app/views/empresas/adminEmpresa.xhtml" />
    </url-mapping>  
    <!--fin empresa-->
    <!--fin graduado-->
    <url-mapping id="adminGraduado">
        <pattern value="/adminGraduado"/>
        <view-id value="/app/views/graduados/adminGraduado.xhtml" />
    </url-mapping>  
    <!--fin graduado-->
    
